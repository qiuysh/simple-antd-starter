{
  "name": "react_system",
  "version": "1.0.0",
  "description": "react+ts+react-router+antd+webpack+redux",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --profile --config scripts/prod.js",
    "start": "cross-env NODE_ENV=development webpack serve --progress --config scripts/dev.js",
    "lint:test": "eslint src --ext .js,.ts,.tsx",
    "lint:fix": "eslint src --fix --ext .js,.ts,.tsx",
    "prettier": "prettier --write src/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": "git+https://github.com/qiuysh/react_system.git",
  "author": "wulin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qiuysh/react_system/issues"
  },
  "homepage": "https://github.com/qiuysh/react_system#readme",
